{
  "name": "lifo-app-server",
  "version": "0.0.0",
  "description": "The server portion of the Lifo Service",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "mocha --recursive --require mocha-istanbul 'test' && istanbul report text-summary lcov",
    "lint": "eslint server.js **/*.js",
    "report": "istanbul report",
    "report-summary": "istanbul report text-summary lcov",
    "codecov": "mocha --recursive --require mocha-istanbul 'test' && istanbul report && codecov",
    "nodemon": "nodemon -e js,json,html,css server.js"
  },
  "engines": {
    "node": "7.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LyneApp/lyne-server.git"
  },
  "keywords": [
    "Lyne",
    "Queue",
    "Lyn",
    "Quyk",
    "Lifo"
  ],
  "author": {
    "name": "Edward Seim",
    "email": "sirseim@gmail.com",
    "url": "http://sirseim.github.io/"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/LyneApp/lyne-server/issues"
  },
  "homepage": "https://github.com/LyneApp/lyne-server#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-istanbul": "^0.3.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "good": "^7.1.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.1",
    "hapi": "^16.1.0",
    "inert": "^4.1.0",
    "nunjucks-hapi": "^2.1.0",
    "vision": "^4.1.1"
  }
}
